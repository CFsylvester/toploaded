{
  "name": "next-tailwind-typescript-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check-format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "db:push": "drizzle-kit push",
    "db:gen": "drizzle-kit generate",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:link": "supabase link --project-ref $SUPABASE_PROJECT_REF",
    "db:types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_REF --schema public --schema auth --schema storage > supabase/types/database.ts",
    "pokemon:set": "tsx src/lib/pokemon/index.ts",
    "pokemon:sprites:sync": "tsx src/lib/pokemon/sprites/index.ts",
    "cloudflare:delete:bucket": "tsx src/core/cloudflare/operations/deleteFolder.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.888.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@types/papaparse": "^5.3.15",
    "change-case": "^5.4.4",
    "cheerio": "^1.1.2",
    "csv-parse": "^5.6.0",
    "lucide-react": "^0.510.0",
    "next": "15.3.1",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.2",
    "pg": "^8.15.6",
    "pokemon-tcg-sdk-typescript": "^1.3.4",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-csv": "^2.2.2",
    "react-dom": "^19.0.0",
    "sass": "^1.69.5",
    "sharp": "^0.34.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@types/pg": "^8.15.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sharp": "^0.32.0",
    "autoprefixer": "^10.4.14",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.43.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "postcss": "^8.4.21",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.19.4",
    "typescript": "^5"
  },
  "packageManager": "yarn@1.22.22"
}
